@misc{zhangWoCoCoLearningWholeBody2024,
  title = {{{WoCoCo}}: {{Learning Whole-Body Humanoid Control}} with {{Sequential Contacts}}},
  shorttitle = {{{WoCoCo}}},
  author = {Zhang, Chong and Xiao, Wenli and He, Tairan and Shi, Guanya},
  year = {2024},
