@misc{luoPreciseDexterousRobotic2024,
  title = {Precise and {{Dexterous Robotic Manipulation}} via {{Human-in-the-Loop Reinforcement Learning}}},
  author = {Luo, Jianlan and Xu, Charles and Wu, Jeffrey and Levine, Sergey},
